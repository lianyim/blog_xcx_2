page {
  background-color: #f3f3f3;
}
.content {
  padding-bottom: 40rpx;
}
.top-background {
  height: 300rpx;
  background-color: #00a5f7;
  text-align: center;
  padding-left: 40rpx;
  padding-top: 50rpx;
}
.top-background .integral-view {
  height: 170rpx;
  color: #FFFFFF;
}
.top-background .integral-view .integral-left {
  float: left;
}
.top-background .integral-view .integral-left .integral {
  font-size: 60rpx;
  margin-bottom: 10rpx;
  font-weight: bold;
  text-align: left;
}
.top-background .integral-view .integral-left .get-integral {
  width: 42rpx;
  height: 42rpx;
  float: left;
  margin-left: 35rpx;
  margin-top: -7rpx;
}
.top-background .integral-view .integral-right {
  float: right;
}
.top-background .integral-view .integral-right .exchange {
  color: #ffaa00;
  background-color: #ffffff;
  border-radius: 30% 0 0 30%;
  margin-top: 40rpx;
  font-size: 28rpx;
  padding-left: 16rpx;
  padding-right: 16rpx;
  padding-top: 6rpx;
  padding-bottom: 6rpx;
}
.list-view {
  width: 78%;
  min-height: 100vh;
  margin: 0 auto;
  margin-top: -100rpx;
  background-color: #ffffff;
  box-shadow: 0 0 2rpx 2rpx #fdfdfd;
  border-radius: 10rpx;
  padding-top: 20rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  padding-bottom: 20rpx;
}
.list-view .data-view {
  height: 110rpx;
  margin-top: 20rpx;
  border-bottom: 2rpx solid #c1c1c1;
}
.list-view .data-view .data-left {
  float: left;
}
.list-view .data-view .data-left .title {
  font-size: 30rpx;
}
.list-view .data-view .data-left .time {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #778288;
  float: left;
}
.list-view .data-view .data-right {
  float: right;
  color: #ff0000;
}
.box-warp {
  background: #fff;
  color: #909090;
  margin-bottom: 36rpx;
}
.box-warp .orderbox > view:nth-child(1) {
  margin-top: 20rpx;
}
.box-warp .type-item-spec {
  padding: 15rpx;
  display: flex;
  margin-left: -60rpx;
}
.box-warp .type-item-spec .dis {
  display: block;
}
.box-warp .type-item-spec .btna {
  color: #FFFFFF;
  font-weight: normal;
}
.box-warp .type-item-spec .btna .type-name {
  border-bottom: 4rpx solid #0079FF;
  height: 46rpx;
  color: #000000;
  font-weight: bold;
  margin-left: 130rpx;
}
.box-warp .type-name {
  height: 46rpx;
  color: #000000;
  margin-left: 130rpx;
}
.cue-words {
  text-align: center;
  font-size: 28rpx;
  color: #778288;
  margin-top: 12rpx;
}
uni-popup .x {
  position: absolute;
  right: 32rpx;
  top: 8rpx;
  color: #fa0000;
  font-size: 40rpx;
}
uni-popup .popup-title {
  text-align: center;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
}
uni-popup .popup-content {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  padding: 15px;
  width: 300px;
  background-color: #f1f1f1;
  font-size: 28rpx;
  letter-spacing: 2rpx;
}
uni-popup .btn-view {
  height: 30rpx;
}

