<view class="content"><view class="trends-outside-view"><view class="trends-top-title">我发布的动态</view><block wx:for="{{trendsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="trends-view"><view class="user-view"><image src="{{item.headUrl}}" mode="aspectFill"></image><view class="username-view"><view class="username">{{item.userName}}</view><view class="time">{{item.createDate}}</view></view></view><view data-event-opts="{{[['tap',[['toTrendsDetail',['$0'],[[['trendsList','',index,'id']]]]]]]}}" class="trends-content" bindtap="__e">{{item.content}}</view><view class="img-view"><block wx:for="{{item.pictureList}}" wx:for-item="e" wx:for-index="i" wx:key="i"><image class="trends-img" mode="aspectFill" src="{{e}}" data-event-opts="{{[['tap',[['previewImage',['$0',i],[[['trendsList','',index,'pictureList']]]]]]]}}" bindtap="__e"></image></block></view></view></block></view><block wx:if="{{trendsList==null||trendsList.length==0}}"><view class="no-data-view">暂无内容</view></block></view>