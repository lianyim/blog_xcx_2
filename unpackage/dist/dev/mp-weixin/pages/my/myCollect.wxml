<view class="content"><view class="article-top-title">我收藏的文章</view><block wx:for="{{articleList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toArticleDetail',['$0'],[[['articleList','',index,'id']]]]]]]}}" class="article-view" bindtap="__e"><view class="article-left"><view class="title">{{item.title}}</view><block wx:if="{{item.topicName!==null}}"><view class="topic-name">{{item.topicName+"️"}}</view></block><block wx:if="{{item.topicName===null}}"><view style="height:16px;"></view></block><view class="desc">{{item.remark}}</view><view class="time">{{item.createDate}}</view></view><view class="article-right"><image mode="aspectFit" src="{{item.coverImg}}"></image></view></view></block><block wx:if="{{articleList==null||articleList.length==0}}"><view class="no-data-view">暂无内容</view></block></view>